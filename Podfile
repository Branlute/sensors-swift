    platform :osx, '10.10'
    
    pod 'Signals', :git => 'https://github.com/netizen01/Signals.git'
end

target 'SwiftySensorsExample' do
    platform :ios, '8.4'
    
    pod 'Signals', :git => 'https://github.com/netizen01/Signals.git'
end
